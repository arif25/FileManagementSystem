<Window x:Class="FileManagementSystem.CreateLibrary"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Create Library" Height="265" Width="542">
    <Grid>
        <Label Content="Name" Height="28" HorizontalAlignment="Left" Margin="38,40,0,0" Name="label1" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="108,44,0,0" Name="textBox1" VerticalAlignment="Top" Width="255" />
        <Label Content="Path" Height="28" HorizontalAlignment="Left" Margin="46,104,0,0" Name="label2" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="108,104,0,0" Name="textBox2" VerticalAlignment="Top" Width="255" />
        <Button Content="Browse" Height="23" HorizontalAlignment="Left" Margin="395,104,0,0" Name="browseBtn" VerticalAlignment="Top" Width="75" Click="browseBtn_Click" />
        <Button Content="Create" Height="34" HorizontalAlignment="Left" Margin="85,170,0,0" Name="createBtn" VerticalAlignment="Top" Width="155" Click="createBtn_Click" />
        <Button Content="Cancel" Height="34" HorizontalAlignment="Left" Margin="315,170,0,0" Name="cancelBtn" VerticalAlignment="Top" Width="155" Click="cancelBtn_Click" />
    </Grid>
</Window>
